{
    "name": "Second Choice ",
    "version": "1.0",
    "description": "Build an Extension!",
    "browser_action": {
        "default_popup": "popup_2.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "24": "icons/icon24.png",
            "32": "icons/icon32.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["declarativeContent", "storage", "activeTab"],
    "manifest_version": 2
}